{
  "name": "@shockproofai/ai-chatbot-component",
  "version": "1.0.0",
  "description": "Reusable AI Chatbot Component for ShockProof AI applications",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shockproofai/ai-chatbot-component.git"
  },
  "keywords": [
    "react",
    "chatbot",
    "ai",
    "firebase",
    "typescript"
  ],
  "author": "ShockProof AI",
  "license": "MIT",
  "peerDependencies": {
    "firebase": ">=10.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "firebase": {
      "optional": false
    }
  },
  "dependencies": {
    "@tanstack/react-query": "^5.0.0",
    "lucide-react": "^0.400.0",
    "react-markdown": "^9.0.0",
    "remark-gfm": "^4.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0"
  }
}